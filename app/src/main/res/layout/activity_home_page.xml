<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background03_blur"
    tools:context=".Home_page">

    <TextView
        android:id="@+id/welcome01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/basic"
        android:text="Hello,"
        android:textColor="#475FE1"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/username_hometext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/username_hometext"
        android:layout_width="359dp"
        android:layout_height="57dp"
        android:gravity="start"
        android:text="User Name"
        android:textColor="#D5B2B2"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.794"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.101" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="421dp"
        android:layout_height="55dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/dock_pic" />

    <ImageView
        android:id="@+id/tx_icon_button"
        android:layout_width="97dp"
        android:layout_height="96dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eventImageSlider"
        app:layout_constraintVertical_bias="0.996"
        app:srcCompat="@drawable/txicon" />

    <ImageView
        android:id="@+id/profile_ico"
        android:layout_width="26dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_btn"
        app:layout_constraintEnd_toEndOf="@+id/profile_btn"
        app:layout_constraintHorizontal_bias="0.38"
        app:layout_constraintStart_toStartOf="@+id/profile_btn"
        app:layout_constraintTop_toTopOf="@+id/profile_btn"
        app:layout_constraintVertical_bias="0.2"
        app:srcCompat="@drawable/profile_icon" />

    <ImageView
        android:id="@+id/inbox_ico"
        android:layout_width="26dp"
        android:layout_height="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/inbox_btn"
        app:layout_constraintEnd_toEndOf="@+id/inbox_btn"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/inbox_btn"
        app:layout_constraintTop_toTopOf="@+id/inbox_btn"
        app:layout_constraintVertical_bias="0.2"
        app:srcCompat="@drawable/inbox_icon" />

    <ImageView
        android:id="@+id/live_ico"
        android:layout_width="24dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/gallery_btn"
        app:layout_constraintEnd_toEndOf="@+id/gallery_btn"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/gallery_btn"
        app:layout_constraintTop_toTopOf="@+id/gallery_btn"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/ic_baseline_live_tv_24" />

    <TextView
        android:id="@+id/profile_txt"
        android:layout_width="41dp"
        android:layout_height="19dp"
        android:text="Profile"
        android:textColor="#3777AA"
        app:layout_constraintBottom_toBottomOf="@+id/profile_btn"
        app:layout_constraintEnd_toEndOf="@+id/profile_btn"
        app:layout_constraintStart_toStartOf="@+id/profile_btn"
        app:layout_constraintTop_toTopOf="@+id/profile_btn"
        app:layout_constraintVertical_bias="0.81" />

    <TextView
        android:id="@+id/inbox_txt"
        android:layout_width="42dp"
        android:layout_height="20dp"
        android:text="Inbox"
        android:textColor="#3777AA"
        app:layout_constraintBottom_toBottomOf="@+id/inbox_btn"
        app:layout_constraintEnd_toEndOf="@+id/inbox_btn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/inbox_btn"
        app:layout_constraintTop_toTopOf="@+id/inbox_btn"
        app:layout_constraintVertical_bias="0.8"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="781dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="52dp"
        android:layout_height="19dp"
        android:text="Gallery"
        android:textColor="#3777AA"
        app:layout_constraintBottom_toBottomOf="@+id/gallery_btn"
        app:layout_constraintEnd_toEndOf="@+id/gallery_btn"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@+id/gallery_btn"
        app:layout_constraintTop_toTopOf="@+id/gallery_btn"
        app:layout_constraintVertical_bias="0.75" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="39dp"
        android:layout_height="17dp"
        android:text="Rank"
        android:textColor="#3777AA"
        app:layout_constraintBottom_toBottomOf="@+id/rank_btn"
        app:layout_constraintEnd_toEndOf="@+id/rank_btn"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@+id/rank_btn"
        app:layout_constraintTop_toTopOf="@+id/rank_btn"
        app:layout_constraintVertical_bias="0.7"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="782dp" />

    <ImageView
        android:id="@+id/rank_ico"
        android:layout_width="35dp"
        android:layout_height="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/rank_btn"
        app:layout_constraintEnd_toEndOf="@+id/rank_btn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/rank_btn"
        app:layout_constraintTop_toTopOf="@+id/rank_btn"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/rankicon" />

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/eventImageSlider"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="148dp"
        android:layout_marginBottom="355dp"
        android:paddingStart="80dp"
        android:paddingEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/profile_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.086" >

    </androidx.viewpager2.widget.ViewPager2>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/createSlider"
        android:layout_width="394dp"
        android:layout_height="180dp"
        android:layout_marginTop="80dp"
        android:paddingStart="90dp"
        android:paddingEnd="90dp"
        app:layout_constraintBottom_toTopOf="@+id/tx_icon_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/eventImageSlider"
        app:layout_constraintVertical_bias="0.0" >

    </androidx.viewpager.widget.ViewPager>

    <Button
        android:id="@+id/profile_btn"
        android:layout_width="47dp"
        android:layout_height="55dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/inbox_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:rippleColor="#E2673AB7" />

    <Button
        android:id="@+id/inbox_btn"
        android:layout_width="45dp"
        android:layout_height="54dp"
        android:layout_marginStart="28dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tx_icon_button"
        app:layout_constraintTop_toTopOf="@+id/profile_btn"
        app:layout_constraintVertical_bias="0.941"
        app:rippleColor="#E2673AB7" />

    <Button
        android:id="@+id/rank_btn"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tx_icon_button"
        app:layout_constraintHorizontal_bias="0.324"
        app:layout_constraintStart_toEndOf="@+id/gallery_btn"
        app:layout_constraintTop_toTopOf="@+id/gallery_btn"
        app:layout_constraintVertical_bias="0.0"
        app:rippleColor="#E2673AB7" />

    <Button
        android:id="@+id/gallery_btn"
        android:layout_width="49dp"
        android:layout_height="53dp"
        android:layout_marginStart="12dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:rippleColor="#1971B8" />

    <Button
        android:id="@+id/settings_btn"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:background="@drawable/ic_baseline_settings_24"
        app:backgroundTint="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.945"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.035" />

</androidx.constraintlayout.widget.ConstraintLayout>